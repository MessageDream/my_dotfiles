[push]
		default = simple
[user]
		email =jaydenzhao@outlook.com
		name = jayden
[alias]
		co = checkout
		br = branch
		ci = commit -a -m
		rb = rebase
		mg = merge
		st = status
		last = log -1 HEAD
		unstage = reset HEAD --
		d = difftool
[color]
		diff = auto
		status = auto
		branch = auto
		interactive = auto
		ui = auto
[color "branch"]
		current = yellow reverse
		local = yellow
		remote = green
[color "diff"]
		meta = yellow bold
		frag = magenta
		plain = white bold
		old = red bold
		new = green
		commit = yellow bold
		func = green dim
[color "status"]
		added = yellow
		changed = green
		untracked = cyan
[core]
		editor = vim
		excludesfile = \"$HOME\"/.gitignore
[diff]
		tool = vimdiff
[difftool]
		prompt = true
[merge]
		tool = vimdiff3
[mergetool]
		prompt = true
		keepBackup = false
[mergetool "vimdiff3"]
		cmd = vim -f -d \"$LOCAL\" \"$MERGED\" \"$REMOTE\"
#[difftool "sourcetree"]
